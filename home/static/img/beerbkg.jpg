<script>
    $(document).ready(function() {
        if (!$(".new_rdv_layout").val()) {
            const slotUrl = new URL(`${finalApi}`);
            console.log("new_res_finalApi : ", slotUrl);

            function dateTimeChange() {
                let _date = $("[name='date']").val();
                let _time = $("[name='time']").val();
                let _pax = $("[name='pax']").val();

                console.log("change : ", $("[name='date']").val(), $("[name='time']").val(), $("[name='pax']")
                    .val());

                let deal_types = 'pospaid'; // testing purpose (will delete later)

                // const slotUrl =
                //     `https:/ns-dev.eazydiner.com/5.2/restaurants/${restaurant_data?.action_url}/slots/${_date}/${_time}`;
                // slotUrl = `${slotUrl}?pax=${_pax}&${deal_types}`;

                slotUrl.searchParams.set("date", _date);
                slotUrl.searchParams.set("time", _time);
                slotUrl.searchParams.set("pax", _pax);

                if ($(".deal_types_value").val()) {
                    slotUrl.searchParams.set("deal_types[]", $(".deal_types_value").val());
                }
                window.slotUrlCall(`${slotUrl.href}`);
            }
            $(".slot_datepicker").Zebra_DatePicker({
                container: $(".inner_markup"),
                direction: ['16 Jan, 2024', 30],
                format: "d M, Y",
                onSelect: function(dateChange) {
                    const dateFormated = moment(dateChange).format("YYYY-MM-DD");
                    $("[name='date']").val(dateFormated)
                    dateTimeChange();
                }
            });

            showSlotDeal = (showDeal) => {
                const dealId = $(showDeal).data("toggleDeal");
                $(".deal_main .deal_box_main").addClass("hidden-notImp");
                $(".deal_main [data-toggle-id='" + dealId + "']").removeClass("hidden-notImp");
            }

            $(document).on("click", ".slot_div", function() {
                showSlotDeal($(this))
                $(".slot_box .slot_div").removeClass("selected_slot");
                $(this).addClass("selected_slot");
                $("[name='time']").val($(this).data("toggle-deal"));
                dealSelection($(this));
            })

            showSlotDeal($("[data-is-selected='1']"))

            $("#select_guest").change(function() {
                if ($("#select_guest option:selected").hasClass("extra-guests-option")) {
                    $(".extra_guests").addClass("active");
                } else {
                    $("[name='pax']").val($(this).val())
                    dateTimeChange();
                }
            })

            $(document).on("click", ".select_deal", function() {
                dealSelection($(this));
            })

            function checkDeal() {
                if (!$(".main_book > div").hasClass("login-signup")) {
                    if ($("[name='deal_id']").val() != "") {
                        $(".main_book").removeClass("disable_book")
                    } else {
                        $(".main_book").addClass("disable_book")
                    }
                }
            }

            function dealSelection(element) {
                const findElement = $(element).data("toggle-deal");
                if ($(element).text() == "Select") {
                    $(element).parents('.deal_box_main').find(".deal_box").removeClass("selected_deal")
                    $(element).parents("[data-deal-box='" + findElement + "']").addClass("selected_deal");
                    $(element).parents('.deal_box_main').find(".select_deal").text("Select");
                    $(element).text("Selected");
                } else {
                    $("[data-toggle-id='" + findElement + "'] .selected_deal .select_deal").text("Selected");
                }
                dealIdCoupons($("[data-toggle-id='" + findElement + "'] .selected_deal").data("deal-id"))
                checkDeal();
            }

            dealSelection($(".selected_slot"));

            $(".select_qsr").click(function() {
                plusMinus($(this), "qsr")
            })

            $(".minus_plus").click(function() {
                const getUpdateVal = $(this).parents(".qsr_main").find(".value");
                if ($(getUpdateVal).data("value") < $(getUpdateVal).data("max-pax") || $(this).data(
                        "operator") == "minus") {
                    plusMinus($(this))
                }
            })

            function plusMinus(element, selectBtn) {
                const getOperator = $(element).data("operator");
                const getNumValue = $(element).parents(".qsr_main").find(".value");
                const getValue = $(getNumValue).data("value");
                $(".deal_box .qsr_main .value").data("value", 0);
                const plusMinusVal = getOperator == "plus" ? getValue + 1 : getValue - 1;
                $(getNumValue).data("value", plusMinusVal)
                $(getNumValue).text(plusMinusVal);
                if (selectBtn == "qsr") {
                    $(".deal_box .qsr_main").removeClass("and_remove");
                    $(element).parents(".qsr_main").addClass("and_remove");
                }

                if (plusMinusVal == 0) {
                    $(element).parents(".qsr_main").removeClass("and_remove")
                }

                if (plusMinusVal > 0) {
                    $(".voucher_text").text(
                        `Continue to buy ${plusMinusVal} ${plusMinusVal > 1 ? 'vouchers' : 'voucher'}`);
                } else {
                    $(".voucher_text").text(`Select voucher to continue`);
                }
                dealIdCoupons($(element).data("qsr-id"), plusMinusVal);
                if (plusMinusVal == 0) {
                    $("[name='deal_id']").val("");
                }
                checkDeal();
            }

            function dealIdCoupons(...couponDealId) {
                const [dealId, couponCount] = couponDealId;
                // console.log(dealId, couponCount)
                dealId ? $("[name='deal_id']").val(dealId) : false;
                couponCount ? $("[name='coupons']").val(couponCount) : false;
            }

            $(".deal_bucket_box").click(function() {
                $("html, body").animate({
                    'scrollTop': $(".slots_main").position().top
                })
                const popupAppend = $(this).data("link");
                window.slotUrlCall(popupAppend);
                $(".deal_types_value").val($(this).data("dealtype"));
            })

            $(".inner_markup .fancy_modal").click(function() {})


        }

        $(".popup.extra_guests").on('click', '.extra_guest_cta a', () => {
            if ($(this).attr("href") == "#login") {
                $(".extra_guests").removeClass("active");
                return;
            }

            var requestType = $(this).attr("href") == "#request" ? "request" : "query";

            var userId = $("input[name=customer_id]").val();
            var restaurantId = $("input[name=restaurant_id]").val();
            var date = $(".input_date input[name=date]").val();
            var time = $(".input_date input[name=time]").val();
            var pax = $(".input_date input[name=pax]").val();
            const method = "POST";
            const endPoint = "/group-booking";
            $.ajax({
                type: method,
                url: endPoint,
                headers: {
                    "X-CSRF-TOKEN": $('meta[name="csrf-token"]').attr("content"),
                },
                data: {
                    type: requestType,
                    user_id: userId,
                    restaurant_id: restaurantId,
                    pax: pax,
                    slot: time,
                    date: date,
                },
                success: (data) => {
                    var msg = data.data.msg;
                    var requestID = data.data.request_id;
                    var responseMsg = `${msg}. Reference no. ${requestID}`;
                    $(".request-submitted-success p").html(responseMsg);
                    $(".request-submit").css("display", "none");
                    $(".request-submitted-success ").css("display", "block");
                    // $("#select_guest")[0][0].selected = true;
                },
                error: (err) => {
                    // $("#select_guest")[0][0].selected = true;
                    console.log(err.responseJSON.message);
                },
            });
        });

        $(".popup.extra_guests").on('click', ".request-submitted-success a, .close-btn", () => {
            $(".extra_guests").removeClass("active");
            // $("#select_guest")[0][0].selected = true;
        });

        $(".popup.extra_guests").on("click", ".extra_guest_cta .login-trigger", () => {
            $(".close-btn").click();
            $("#login-btn").click();
        });

        $('.request-submitted-success a, .close-btn').click(() => {
            $('.extra_guests').removeClass("active");
            $('#select_guest')[0][0].selected = true;
        });

        $('.offer_details').on('click', '.info_content_offer', function(e) {
            console.log("info content offer clicked...", $(this));
            $('.info_content_offer.active').removeClass('active');
            $('.info_content_offer').closest('.offer_details').find('.info_content').not('._hidden')
                .addClass('_hidden');

            console.log($(this), $(this).closest('.offer_details').find('.info_content'));

            if ($(this).hasClass('res_offer')) {
                if (!$(this).hasClass('active')) {
                    $(this).addClass('active');
                }
                $(this).closest('.offer_details').find('.info_content.res_offer').removeClass('_hidden')

            } else if ($(this).hasClass('pay_offer')) {
                if (!$(this).hasClass('active')) {
                    $(this).addClass('active');
                }
                $(this).closest('.offer_details').find('.info_content.pay_offer').removeClass('_hidden')
            }
        })

        $('.res_deal_info_container').on('click',
            '._restaurant_offer_heading, ._payment_offer_heading,.offer_box_round, .offer_boxes, .payment_offer .view_all_link',
            function(e) {
                console.log("web all popup -> offer_details_icon -> called...");
                let deal_box_id = $(this).closest('.deal_box').attr('data-deal-id');
                console.log("on click : ", $(this), $(this).closest('.deal_box'), deal_box_id);
                let test = $('.offer_details').filter((index, item) => {
                    console.log("checking offer_details_icon_index : ", deal_box_id, );
                    return $(item).hasClass(`offer_details_${deal_box_id}`);
                })
                console.log("check -> ", test, deal_box_id);

                if ($(this).data('name') == 'res_offer_info') {
                    $(test).find('.info_content_offer.res_offer').click();
                } else if ($(this).data('name') == 'pay_offer_info') {
                    $(test).find('.info_content_offer.pay_offer').click();
                }
            })

        $('.web_all_popup').on('click', '.view_all_payment_offer', function(e) {
            console.log("view_all_payment_offer clicked...");
            all_payment_offer_class = $(this).attr('data-popup');
            $(all_payment_offer_class).find('#search_payment_offer').val("").trigger('input');
        });
    })
</script>
