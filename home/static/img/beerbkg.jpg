            let total_bill = $('.enter_total_bill').val() ? parseInt($('.enter_total_bill').val()) : 0;
            let res_offer = 0;
            let pay_offer = 0;

            let resOfferCalc = $('input[name=web_selected_restaurant_offer]').attr('data-calculation') ?
                JSON.parse($('input[name=web_selected_restaurant_offer]').attr('data-calculation')) :
                "";
            let payOfferCalc = $('input[name=web_selected_payment_offer]').attr('data-calculation') ?
                JSON.parse($('input[name=web_selected_payment_offer]').attr('data-calculation')) : "";

            // console.log(total_bill, resOfferCalc, payOfferCalc);
            if (total_bill && (resOfferCalc || payOfferCalc)) {

                // // use-case : when any payment offer alredy selcted by default and after that use enter total_bill value which is lesser than min_transaction amount - then we need to disable calculate button
                // if(payOfferCalc?.min_transaction_amt && total_bill < payOfferCalc?.min_transaction_amt){
