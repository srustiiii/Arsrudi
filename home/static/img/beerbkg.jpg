                    }

                    /***** sorting elements (selected, active, disabled)  *****/
                    let arr = $('._payment_offer_wrapper_container').children();
                    // empty the payement _payment_offer_wrapper_container
                    $('._payment_offer_wrapper_container').empty()

                    // sorting elements (selected, active, disabled)
                    let sorted_arr = sortOnMinBill(arr, _value);

                    // append the sorted array
                    $('._payment_offer_wrapper_container').append(sorted_arr);

                    // after sorting -->  select the first offer by default
                    setTimeout(() => {
                        $($('._payment_offer_wrapper_container').children()[0]).find('.select_offer')
                            .click();
                        $('._payment_offer_wrapper_container').animate({
                            scrollTop: 0
                        });

                    }, 300);


                    return;
                }

                if (_value >= 100) {
                    $('.bill_calculator_content_wrapper ._min_transaction_amt_check_wrapper ._min_transaction_amt_check_wrapper_text')
                        .text('');
                } else {
                    $('.bill_calculator_content_wrapper ._min_transaction_amt_check_wrapper ._min_transaction_amt_check_wrapper_text')
                        .text('Enter minimum bill value of ₹100');
                }

                /***** sorting elements (selected, active, disabled)  *****/
                let arr = $('._payment_offer_wrapper_container').children();
                // empty the payement _payment_offer_wrapper_container
                $('._payment_offer_wrapper_container').empty()

                // sorting elements (selected, active, disabled)
                let sorted_arr = sortOnMinBill(arr, _value);

                // append the sorted array
                $('._payment_offer_wrapper_container').append(sorted_arr);

                // console.log("after sort : ", arr);

                // arr.map(function(index, item){
                //     if($(item).find('._payment_offer_wrapper_wrap').find('button.select_offer').hasClass('pay_offer_selected')){
                //         // selected -> @ top
                //         console.log("selected");
                //     }else if(!$(item).find('._payment_offer_wrapper_wrap').hasClass('_disabled') && !$(item).find('._payment_offer_wrapper_wrap').find('button.select_offer').hasClass('pay_offer_selected')){
                //         // @ middle
