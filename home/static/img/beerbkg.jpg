        function throttleFunc(func, delay) {
            let prev = 0;
            return function(...args) {
                let now = new Date().getTime();

                // console.log(now-prev, delay);

                if (now - prev > delay) {
                    prev = now;
                    return func(...args);
                }
            }
        }

        $('._calculate_btn_wrapper').click(function(e) {
            // e.preventDefault();
            // console.log('_calculate_btn_wrapper clickedd........outside');
            if ($(
                    '.bill_calculator_content_wrapper ._min_transaction_amt_check_wrapper ._min_transaction_amt_check_wrapper_text'
                )
                .text() != '') {
                $('.selectedMenu').removeClass('selectedMenu');
                $('.bill_calculator_content_wrapper ._min_transaction_amt_check_wrapper').toggleClass(
                    '_hidden');

                setTimeout(() => {
                    if (!$(
