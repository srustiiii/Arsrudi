                $('._payment_offer_wrapper ._offer_text').map(function(index, item) {
                    if (!$(item).text().toLowerCase().includes(e.target.value.toLowerCase())) {
                        if (!$(item).parent('.payment_offer_wrapper_wrap').parent(
                                '._payment_offer_wrapper').hasClass('_hidden')) {
                            $(item).parent('.payment_offer_wrapper_wrap').parent(
                                '._payment_offer_wrapper').addClass('_hidden');
                        }


                    } else {
                        if ($(item).parent('.payment_offer_wrapper_wrap').parent(
                                '._payment_offer_wrapper').hasClass('_hidden')) {
                            $(item).parent('.payment_offer_wrapper_wrap').parent(
                                '._payment_offer_wrapper').removeClass('_hidden');
                        }
                    }
                })
                if ($('._payment_offer_wrapper').filter('._hidden').length == $(
                        '._payment_offer_wrapper').length) {
                    // console.log("no offers found");
                    if ($('._no_offers_found').hasClass('_hidden')) {
                        $('._no_offers_found').removeClass('_hidden')
                    }
                } else {
                    // console.log("offers found");
                    if (!$('._no_offers_found').hasClass('_hidden')) {
                        $('._no_offers_found').addClass('_hidden')
                    }
                }
                // console.log($(fArr).addClass('hidden'));
            }
        });
    })
