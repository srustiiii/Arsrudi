                // }
                //     return;
                // }

                // // enable calculate button
                // $('#bill_calculator_popup ._calculate_btn_wrapper ').removeClass('_disabled')

                // getting restaurant offer after restaurant offer calculation
                res_offer = resOfferCalc ? restaurantOfferCalculation(total_bill, resOfferCalc) : 0;

                // total bill - restaurant offer --/// totalBill_resOffer == total_bill (if resOfferCalc is empty)
                let totalBill_resOffer = total_bill - res_offer;

                // getting payment offer after payment offer calculation
                pay_offer = payOfferCalc ? paymentOfferCalculation(total_bill, totalBill_resOffer,
                    payOfferCalc) : 0;

                // getting final amount to be paid  ::  //// totalBill_resOffer == total_bill (if resOfferCalc is empty) /// finalAmountToBePaid == total_bill (if resOfferCalc & payOfferCalc both are empty)
                let finalAmountToBePaid = 0;
                if (Math.ceil(totalBill_resOffer - pay_offer) <= 0) {
                    finalAmountToBePaid = 1;
                    pay_offer--;
                } else if (Math.ceil(totalBill_resOffer - pay_offer) > 0) {
                    finalAmountToBePaid = Math.ceil(totalBill_resOffer - pay_offer);
                }
                // finalAmountToBePaid =  Math.ceil(finalAmountToBePaid);

                // total saving
                let total_saving = res_offer + pay_offer;

                // console.log("new ==> ", total_bill, res_offer, totalBill_resOffer, pay_offer,finalAmountToBePaid, total_saving);

                // discount_calculator_save
                // _total_bill_final
                // _res_offer_final
                // _pay_offer_final
                // _total_paid_final
                // _total_save_final
                // _total_saving_approx

                $('#discount_calculator_save ._total_bill_final').text(total_bill);
                $('#discount_calculator_save ._res_offer_final').text(res_offer);
                $('#discount_calculator_save ._pay_offer_final').text(pay_offer);
                $('#discount_calculator_save ._total_paid_final').text(finalAmountToBePaid);
                $('#discount_calculator_save ._total_save_final').text(total_saving);
                $('#discount_calculator_save ._total_saving_approx').text(total_saving);


