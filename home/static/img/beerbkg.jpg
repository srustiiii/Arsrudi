                        if (_value >= 100) {
                            $('._calculate_btn_wrapper button').removeClass('_disabled_button');
                            // error
                            $('.bill_calculator_content_wrapper ._min_transaction_amt_check_wrapper ._min_transaction_amt_check_wrapper_text')
                                .text('');
                        } else {
                            $('.bill_calculator_content_wrapper ._min_transaction_amt_check_wrapper ._min_transaction_amt_check_wrapper_text')
                                .text('Enter minimum bill value of ₹100');

                            if (!$('._calculate_btn_wrapper button').hasClass('_disabled_button')) {
                                $('._calculate_btn_wrapper button').addClass('_disabled_button')
                            }
                        }
                        $('._payment_offer_wrapper_container').animate({
                            scrollTop: 0
                        });
                        return false;
                        // $(item).click();
                        // break;
                    }
                });

            } else {
                // console.log(_value, " invalid number - entering into else part== <1");

                // deselect button 
                // if button is selected and the wrapper has error_text (not hidden) ==> remove text, deselect button
                if ($('._payment_offer_wrapper_container button.select_offer.pay_offer_selected') && !$(
                        '._payment_offer_wrapper_container button.select_offer.pay_offer_selected').closest(
                        '._payment_offer_wrapper').find('.error_text').hasClass('_hidden')) {
                    $('._payment_offer_wrapper_container button.select_offer.pay_offer_selected').text('');
                    $('._payment_offer_wrapper_container button.select_offer.pay_offer_selected').removeClass(
                        'pay_offer_selected');
                }
                disablePaymentOffer();

                /***** sorting elements (selected, active, disabled)  *****/
                let arr = $('._payment_offer_wrapper_container').children();
                // empty the payement _payment_offer_wrapper_container
                $('._payment_offer_wrapper_container').empty()

                // sorting elements (selected, active, disabled)
                let new_sorted_arr = sortOnMinBill(arr, _value);

                // append the sorted array
                $('._payment_offer_wrapper_container').append(new_sorted_arr);

                // data-selectPayOfferText
                // data-selectPayOfferSubText
                // data-selectPayOfferImg

                // console.log($(arr[0]).find('.select_offer').attr('data-selectPayOfferText'));
                // console.log($(arr[0]).find('.select_offer').attr('data-selectPayOfferSubText'));
                // console.log($(arr[0]).find('.select_offer').attr('data-selectPayOfferImg'));

                $('#web_selected_payment_offer_text').text($(arr[0]).find('.select_offer').attr(
                    'data-selectPayOfferText'));
                $('#web_selected_payment_offer_subtext').text($(arr[0]).find('.select_offer').attr(
                    'data-selectPayOfferSubText'));
                $('#web_selected_payment_offer_img').attr('src', $(arr[0]).find('.select_offer').attr(
