    function sortOnMinBill(arr, _value) {

        let selected_arr = [];
        let indusind_arr = [];
        let trans_meet_arr = [];
        let null_arr = [];
        let trans_not_meet_arr = [];
        arr.each(function(index, item) {
            let _item_calc = $(item).find('button.select_offer').attr('data-selectCalculation') ? JSON.parse($(
                item).find('button.select_offer').attr('data-selectCalculation')) : '';
            // let _item_offer_text = $(item).find('button.select_offer').attr('data-selectpayoffertext') ? JSON.parse($(item).find('button.select_offer').attr('data-selectpayoffertext')) : '';
            // console.log("inside sorting :: ",index, _value, _item_calc?.min_transaction_amt, $(item).find('button.select_offer').attr('data-selectpayoffertext'));

            // if($(item).find('button.select_offer').hasClass('pay_offer_selected')){
            //     selected_arr.push(item);
            // }else 

            if (_item_calc?.min_transaction_amt == null) {
                null_arr.push(item);
                // console.log("In null");
            } else if ($(item).find('button.select_offer').attr('data-selectpayoffertext')?.includes(
                    'EazyDiner IndusInd Bank Card')) {
                indusind_arr.push(item);
                // console.log("In indusind card");
            } else if (_value >= _item_calc?.min_transaction_amt) {
                trans_meet_arr.push(item);
                // console.log("In trans_meet_arr");
            } else if (_value < _item_calc?.min_transaction_amt) {
                trans_not_meet_arr.push(item);
                // console.log("In trans_not_meet_arr");

            }
        })

        let sorted_arr = [...indusind_arr, ...trans_meet_arr, ...null_arr, ...trans_not_meet_arr];
        // let new_calc_arr = [];
        // new_calc_arr.push(indusind_arr);
        // new_calc_arr.push(trans_meet_arr);
        // new_calc_arr.push(null_arr);
        // new_calc_arr.push(trans_not_meet_arr);

        // console.log("selected_arr : ",selected_arr);
        // console.log("Indusind : ",indusind_arr);
        // console.log("Trans_meet_arr : ",trans_meet_arr);
        // console.log("Null arr : ",null_arr);
        // console.log("Trans_not_meet_arr : ",trans_not_meet_arr);

        // console.log("calc_arr : ", sorted_arr);


        return sorted_arr;


    }


    $(document).ready(function() {
        $('#search_payment_offer').on('input', function(e) {
            // console.log(e.target.value);
            // let _searchOffer_input = e.target.value;
            if (e.target.value == '') {
                // $()
                $('#payment_offer_popup ayment_offer_popup ._payment_offer_wrapper').removeClass(
                    '_hidden')
            } else {
                $('#payment_offer_popup ._payment_offer_wrapper ._offer_text').map(function(index,
                    item) {
                    if (!$(item).text().toLowerCase().includes(e.target.value.toLowerCase())) {
                        // console.log($(item).text(), "Not matched");
                        if (!$(item).parent('._payment_offer_wrapper_wrap').parent(
                                '._payment_offer_wrapper').hasClass('_hidden')) {
                            $(item).parent('._payment_offer_wrapper_wrap').parent(
                                '._payment_offer_wrapper').addClass('_hidden');
                        }


                    } else {
                        // console.log($(item).text(), "Matched");
                        if ($(item).parent('._payment_offer_wrapper_wrap').parent(
                                '._payment_offer_wrapper').hasClass('_hidden')) {
                            $(item).parent('._payment_offer_wrapper_wrap').parent(
                                '._payment_offer_wrapper').removeClass('_hidden');
                        }
                    }
                })
                if ($('#payment_offer_popup ._payment_offer_wrapper').filter('._hidden').length == $(
                        '#payment_offer_popup ._payment_offer_wrapper').length) {
                    // console.log("no offers found");
                    if ($('#payment_offer_popup ._no_offers_found').hasClass('_hidden')) {
                        $('#payment_offer_popup ._no_offers_found').removeClass('_hidden')
                    }
                } else {
                    // console.log("offers found");
                    if (!$('#payment_offer_popup ._no_offers_found').hasClass('_hidden')) {
                        $('#payment_offer_popup ._no_offers_found').addClass('_hidden')
                    }
                }
                // console.log($(fArr).addClass('hidden'));
            }
        });

        $('.close-popup_payment_offer').on("click", function() {
            $('._payment_offer_wrapper_container ._payment_offer_wrapper').removeClass('_hidden');
            $('#search_payment_offer').val('');

            $("#payment_offer_popup").removeClass("show_popup");
            // $('body').css('overflow', 'unset');
        });

        $('#payment_offer_popup').on('click', '.select_offer', function(e) {
            // console.log("by default clicking first value : ");

            // let value = $('.enter_total_bill').val();
            let _value = $('.enter_total_bill').val() ? JSON.parse($('.enter_total_bill').val()) : '';

            _res_calc = $('input[name=web_selected_restaurant_offer]').attr('data-calculation') ? JSON.parse($('input[name=web_selected_restaurant_offer]').attr('data-calculation')) : '';
            if (_res_calc?.type?.toLowerCase()?.includes('percentage') && _res_calc?.value) {
                _res_offer = Math.ceil(_value * (_res_calc.value / 100));
                // console.log("Res_offer: ", _res_offer);
                _value = _value - _res_offer;
            }

            $('#payment_offer_popup .select_offer').removeClass('pay_offer_selected');
            $('#payment_offer_popup .select_offer').map((index, item) => {
                $(item).text('Select');
                if ($(item).hasClass('pay_offer_selected')) {
                    $(item).removeClass('pay_offer_selected');
                }
            })

            // console.log($(this).attr('data-selectResOffer'));
            $('input[name=web_selected_payment_offer]').attr('data-offer', $(this).attr(
                'data-selectPayOfferText'));
            $('input[name=web_selected_payment_offer]').attr('data-subOffer', $(this).attr(
                'data-selectPayOfferSubText'));
            $('input[name=web_selected_payment_offer]').attr('data-calculation', $(this).attr(
                'data-selectCalculation'));

            $('input[name=web_selected_payment_offer]').attr('data-offerImg', $(this).attr(
                'data-selectPayOfferImg'));


            $(this).addClass('pay_offer_selected');
            $(this).text('Selected');
            // $(this).css({"border":'2px solid green', "pointer-events":"none"})
            // $(this).parent('.box_wrapper._offer_wrapper').css();
            // console.log($());
            $('#web_selected_payment_offer_text').text($('input[name=web_selected_payment_offer]').attr(
                'data-offer'))
            $('#web_selected_payment_offer_subtext').text($('input[name=web_selected_payment_offer]')
                .attr('data-subOffer'))
            $('#web_selected_payment_offer_img').attr('src', $('input[name=web_selected_payment_offer]')
                .attr('data-offerImg'));

            if ($('#web_selected_payment_offer_img').parent('.img_wrap').hasClass('_hidden')) {
                $('#web_selected_payment_offer_img').parent('.img_wrap').removeClass('_hidden');
            }

            /***** sorting colors (selected, active, disabled)  *****/
            let arr = $('._payment_offer_wrapper_container').children();
            // empty the payement _payment_offer_wrapper_container
            $('._payment_offer_wrapper_container').empty();
            // sorting colors (selected, active, disabled)
            let sorted_arr = sortOnMinBill(arr, _value);

            // console.log("Checking arr : ");

            // append the sorted array
            $('._payment_offer_wrapper_container').append(sorted_arr);

            $('._payment_offer_wrapper_container').animate({
                scrollTop: 0
            });

            let _calc = $('input[name=web_selected_payment_offer]').attr('data-calculation') ? JSON
                .parse($('input[name=web_selected_payment_offer]').attr('data-calculation')) : '';

            // console.log("(@by  default clicking)");
            if (_value >= 100 && _value >= _calc?.min_transaction_amt) {
                // console.log("_calculate_btn_wrapper button ---> activating... - (@by  default clicking)")
                $('._calculate_btn_wrapper button').removeClass('_disabled_button');
            } else {
                //
                // console.log("already activated from previous state ....->");
                if (!$('._calculate_btn_wrapper button').hasClass('_disabled_button')) {
                    // console.log("disabling ._calculate_btn_wrapper button  & error text adding --> else part");
                    $('._calculate_btn_wrapper button').addClass('_disabled_button');
                    // error
                    $('.bill_calculator_content_wrapper ._min_transaction_amt_check_wrapper ._min_transaction_amt_check_wrapper_text')
                        .text('Enter minimum bill value of ₹100');
                }
            }



            // close the close-popup
            $('.close-popup_payment_offer').click()
        });
    });
</script>
            

