<script>
 $(document).ready(function() {
    // $('.timing-details').click(function(){
    //     $('.timing-content').toggleClass("show_all_slots");
    // });

    $(window).on("scroll",function() {
        if($(this).scrollTop() > $(".stack_scroll").offset().top - 68){
            $(".stack_scroll").addClass("stop_section");
        }else{
            $(".stack_scroll").removeClass("stop_section");
        }
    });

    $(".review_tab").click(function(){
        $("[data-tab='reviews']").click();
    })

    $(".chain_res").click(function(){
        if($(this).attr("data-chaincheck") == "0"){
            $.ajax($(this).attr("data-chain"), {
                type: 'GET',
                success: function (data) {
                    $(".chain_pop").append(data);
                    $(".chain_res_name").removeClass("visibility-hidden");
                    $(".chain_loading_data, .error_chain").addClass("hidden-notImp");
                    $(".chain_res").attr("data-chaincheck", "1");
                    $(".chain_count").text($(".res_chain_count").val());
                },
                error: function (error) {
                    console.log(error.responseJSON.message)
                    $(".error_chain, .chain_loading_data").removeClass("hidden-notImp");
                    $(".chain_loading_data").addClass("hidden-notImp");
                    $(".error_chain").text(error.responseJSON.message)
                }
            })
        }
    })

    $(document).on("keyup", ".search_chain_input", function(){
        const searchVal = $(this).val().toLowerCase();
        $(".res_chain_block").map(function(index, element){
            if($(element).attr("data-reslocation").includes(searchVal)){
                $(element).addClass("show_chain_item");
                $(element).removeClass("hide_chain_item");
            }else{
                $(element).removeClass("show_chain_item");
                $(element).addClass("hide_chain_item");
            }
        })
        if($(".hide_chain_item").length == $(".search_data").attr("data-chaincount")){
            $(".no_chain_data").removeClass("hidden-notImp");
        }else{
            $(".no_chain_data").addClass("hidden-notImp");
        }
        searchVal != "" ? $(".res_info_seprate_main").addClass("hidden-notImp") : $(".res_info_seprate_main").removeClass("hidden-notImp");
    });

    if($(".non_login").length > 0){
        $(".non_login").click(function(){
            $("#login-btn").click();
        })
    }
});
