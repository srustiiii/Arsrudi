                }
                // console.log("removed after calculation...");

            } else {
                if (!$('._calculate_btn_wrapper').hasClass('_disabled')) {
                    $('._calculate_btn_wrapper').addClass('_disabled');
                }
            }
        });
        //enter_total_bill

    })

    function restaurantOfferCalculation(total_bil, res_offer_calc) {
        if (res_offer_calc?.type?.toLowerCase() == 'percentage') {
            let _final_restaurant_offer = (res_offer_calc?.value / 100) * total_bil;
            return Math.ceil(_final_restaurant_offer);
        }
        return 0;
    }

    function paymentOfferCalculation(total_bill, total_resOffer, pay_offer_calc) {
        // console.log("paymentOfferCalculation called", pay_offer_calc);
        if (pay_offer_calc?.type?.toLowerCase() == 'flat') {
            let {
                type,
                value
            } = pay_offer_calc;
            // console.log("flat called", total_bill, total_resOffer, pay_offer_calc);


            // final payment offer
            let _final_payment_offer = total_resOffer < value ? total_resOffer : value;
            return Math.ceil(_final_payment_offer);
        } else if (pay_offer_calc?.type?.toLowerCase() == 'percentage') {
            let {
                type,
                min_transaction_amt,
                upto,
                value
            } = pay_offer_calc;
            // console.log("percentage called", total_bill, total_resOffer, pay_offer_calc);
            if (total_bill >= min_transaction_amt) {
                // console.log("total_bill >= min_transaction_amt");

                // final payment offer
                let _final_payment_offer = ((value / 100) * total_resOffer);
                // console.log("at pay offer :: ", _final_payment_offer);
                _final_payment_offer = Math.ceil(_final_payment_offer);


                // checking the final payment offer whether it is less than the thresold value (upto) or not
                return _final_payment_offer <= upto ? _final_payment_offer : upto;
            }
            // console.log(" outside -- total_bill >= min_transaction_amt");

        }
        // console.log("no flat or percentage - outside")
        return 0;
    }
