                // console.log('array: ',arr);
                // hide  _min_transaction_amt_check_wrapper
                if (!$('#bill_calculator_popup ._min_transaction_amt_check_wrapper').hasClass(
                        '_hidden')) {
                    $('#bill_calculator_popup ._min_transaction_amt_check_wrapper').addClass('_hidden');
