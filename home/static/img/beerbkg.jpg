<script>
    $(document).ready(function() {
        function tabsData(getUrlData, selector, tabsSelector) {
            if ($("[data-tab='" + tabsSelector + "']").attr("data-api") == "0") {
                $.ajax("/" + getUrlData, {
                    type: 'GET',
                    success: function(data) {
                        // $(selector).html("");
                        // console.log("data: ",data);
                        $(selector).append(data);
                        $(selector).removeClass("hidden-notImp");
                        $(".rest_data_loader").addClass("hidden-notImp");
                        $("[data-tab='" + tabsSelector + "']").attr("data-api", 1);
                    },
                    error: function(error) {
                        console.log(error);
                    }
                })
                $(".rest_data_loader").removeClass("hidden-notImp");
            } else {
                // console.log("Ajax Already Done")
            }
        }

        if ($(".tabs_active").length) {
            if ($(".tabs_active").attr("data-tabtype") != "hyperlink") {
                const loadUrl = $(".tabs_active").attr("data-url");
                const loadSelector = $(".tabs_active").attr("data-tab");
                const defaultTabsContent = $("[data-restabs='" + loadSelector + "']");
                tabsData(loadUrl, defaultTabsContent, loadSelector);
            }
            const targetPosition = $(".tabs_active").position().left;
            const targetWidth = $(".tabs_active").outerWidth();
            lineMove(targetPosition, targetWidth);
        }

        $(".tabs_text").click(function() {
            if ($(this).attr("data-tabtype") == "tab") {
                const getUrl = $(this).attr("data-url");
                const getTabSelector = $(this).attr("data-tab");
                const getSelector = $("[data-restabs='" + getTabSelector + "']");
                tabsData(getUrl, getSelector, getTabSelector);
                $("[data-restabs]").addClass("hidden-notImp");
                getSelector.removeClass("hidden-notImp");
                const currentUrl = new URL(window.location.href);
                currentUrl.searchParams.set("tab", getTabSelector);
                window.history.replaceState(null, null, currentUrl);
                $(".tabs_text").removeClass("tabs_active");
                $("[data-tab='" + getTabSelector + "']").addClass("tabs_active");
                $("[data-tab='" + getTabSelector + "']").attr("data-api", 1);
                $("html, body").animate({
                    scrollTop: $(".about_rel_info").offset().top - 60
                }, "slow");
                const targetPosition = $(this).position().left;
                const targetWidth = $(this).outerWidth();
                lineMove(targetPosition, targetWidth);
            } else {
                window.location.href = $(this).attr("data-url");
            }
        })

        function lineMove(targetPosition, targetWidth) {
            $(".div_slide").css({
                "transform": "translateX(calc(" + targetPosition + "px - 8px))",
                "width": targetWidth - 18,
            })
        }
        $(".tabs_text").hover(function() {
            if ($(this).attr("data-tabtype") != "hyperlink") {
                const targetPosition = $(this).position().left;
                const targetWidth = $(this).outerWidth();
                lineMove(targetPosition, targetWidth);
            }
        })
        $(".tabs_text").mouseout(function() {
            const targetPosition = $(".tabs_active").position().left;
            const targetWidth = $(".tabs_active").outerWidth();
            lineMove(targetPosition, targetWidth);
        })
    })
</script>

